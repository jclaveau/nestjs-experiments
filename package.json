{
  "name": "nestjs-experiments",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start": "nest start",
    "start:dev": "yarn docker:up && nest start --watch",
    "start:debug": "yarn docker:up && nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "yarn docker:up && jest",
    "test:watch": "yarn docker:up && jest --watch",
    "test:cov": "yarn docker:up && jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "8.2.5",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "8.2.5",
    "@nestjs/graphql": "9.1.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.1.0",
    "@nestjs/platform-express": "8.2.5",
    "@nestjs/typeorm": "8.0.2",
    "@types/mongodb": "3.6.20",
    "bcrypt": "5.0.1",
    "class-validator": "0.13.2",
    "dotenv": "^11.0.0",
    "env-var": "^7.1.1",
    "github-actions-node-versions": "1.0.3",
    "mongodb": "3.7.3",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.2",
    "ts-md5": "1.2.10",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.0",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.5",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "eslint": "8.6.0",
    "jest": "27.4.7",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "\\.(unit|e2e)-spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ]
  }
}
